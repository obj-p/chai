# Domain for ad-hoc distribution (e.g., your-machine.tailnet.ts.net)
CHAI_DISTRIBUTION_DOMAIN=

# Server URL for iOS app (the HTTPS endpoint for API access)
# Example: https://your-machine.tailnet.ts.net
CHAI_SERVER_URL=

# Apple Developer Team ID (from developer.apple.com > Membership)
CHAI_TEAM_ID=

# Match git repository for certificates/profiles (private repo)
MATCH_GIT_URL=

# Match encryption password (generate strong password, store securely)
MATCH_PASSWORD=

# Apple ID credentials for Fastlane
FASTLANE_USER=
# App-specific password (generate at appleid.apple.com > Sign-In and Security > App-Specific Passwords)
FASTLANE_PASSWORD=

# macOS login password (for non-interactive code signing)
MATCH_KEYCHAIN_PASSWORD=
